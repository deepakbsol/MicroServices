spring:
  application:
    name: gateway
  cloud:    gateway:      routes:      - id: StudentServer
        uri: lb://StudentServer
        
    
eureka:  instance:    prefer-ip-address: true
  client:    fetchRegistry: true
    registerWithEureka: true
    service-url:      defaultzone: http://localhost:8761/eureka
      
server:
  port: 9000
  